#include <stdio.h>

void bolinhas(int vet[], int tam_vetor){

    for(int passadaDoCorredor = 0; passadaDoCorredor < tam_vetor - 1; passadaDoCorredor++){

        for(int num_atual = 0; num_atual < tam_vetor - 1 - passadaDoCorredor; num_atual++){

            if(vet[num_atual]>v[num_atual+1]){
                int guardador = vet[num_atual];
                vet[num_atual] = vet[num_atual+1];
                vet[num_atual+1] = guardador;
            }
        }
    }
}

int main(){
    int vetor[] = {7,8,4,5,2,3};
    int tam_vetor = 6;

    bolinhas(vetor, tam_vetor);

    for(int i = 0; i<tam_vetor; i++){
        printf("%d", vetor[i]);
    }

    return 0;
}
